  object qdata: TUniQuery
    Connection = tabel.server
    SQL.Strings = (
      
        'SELECT aap.*,u.username,IFNULL(ntk.jenis_pembayaran,'#39'-'#39') AS jeni' +
        's_pembayaran,'
      
        'IFNULL(ntk.no_kartu,'#39'-'#39') AS no_kartu,IFNULL(ntk.jumlah,0) AS jum' +
        'lah,IFNULL(ntk.adm,0) AS adm,IFNULL(ntk.bank,'#39'-'#39') AS bank,c.nama'
      
        'FROM `a_accperbaikan_pembayaran` aap JOIN `user` u USING(id_user' +
        ')'
      'JOIN `order_pembelian` op USING(no_order)'
      'JOIN customer c USING(id_customer)'
      'LEFT JOIN `n_temp_kasirdetail` ntk ON aap.no_temp=ntk.no'
      'WHERE status_acc='#39'BELUM DI ACC'#39)
    Left = 624
    Top = 192
